{
  "name": "docker-slim",
  "private": true,
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "@grucloud/core": "^1.39.0",
    "@rails/actioncable": "^6.1.4",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "5.4.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "turbolinks": "^5.2.0",
